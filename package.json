{
  "name": "condo-web",
  "version": "0.1.0",
  "private": true,
  "main": "src/App.tsx",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.15",
    "@types/jest": "^26.0.7",
    "@types/materialize-css": "^1.0.8",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "codecov": "^3.7.2",
    "condo-brain": "git+https://github.com/Condominum/condo-brain.git",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "materialize-css": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-materialize": "^3.9.2",
    "react-scripts": "3.4.1",
    "ts-jest": "^26.1.3",
    "ts-loader": "^8.0.1",
    "typedoc": "^0.17.8",
    "typescript": "~3.9.7",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "docs": "npx typedoc --options ./typedoc.json src; touch docs/.nojekyll",
    "publish-docs": "node ./scripts/gh-pages.js",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "test-watch": "npx jest --watchAll --coverage",
    "build-watch": "npx webpack -w --mode=production",
    "build-production": "npx webpack --mode=production",
    "build-development": "npx webpack --mode=development",
    "build-analyze": "npx webpack --profile --json > stats.json && webpack-bundle-analyzer ./stats.json",
    "dev": "npx webpack-dev-server --config ./webpack.config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "css-loader": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^6.0.0",
    "webpack-dev-server": "^3.11.0"
  }
}
